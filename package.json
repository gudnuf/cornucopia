{
  "name": "cornucopia",
  "version": "0.1.0",
  "description": "Store and synchronize cashu proofs",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
